<h1>Listing ticketnames</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Discount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ticketnames.each do |ticketname| %>
  <tr>
    <td><%= ticketname.name %></td>
    <td><%= ticketname.discount %></td>
    <td><%= link_to 'Show', ticketname %></td>
    <td><%= link_to 'Edit', edit_ticketname_path(ticketname) %></td>
    <td><%= link_to 'Destroy', ticketname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ticketname', new_ticketname_path %>
